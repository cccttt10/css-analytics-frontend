{
    "browserslist": {
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ],
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ]
    },
    "dependencies": {
        "@emotion/core": "^10.0.28",
        "@emotion/styled": "^10.0.27",
        "axios": "^0.19.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "theme-ui": "^0.3.1"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/node": "^12.0.0",
        "@types/react": "^16.9.0",
        "@types/react-dom": "^16.9.0",
        "@types/theme-ui": "^0.3.2",
        "@typescript-eslint/eslint-plugin": "^3.2.0",
        "@typescript-eslint/parser": "^3.2.0",
        "cross-env": "^7.0.2",
        "eslint": "^7.2.0",
        "eslint-plugin-react": "^7.20.0",
        "eslint-plugin-simple-import-sort": "^5.0.3",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.9",
        "prettier": "^2.0.5",
        "react-scripts": "3.4.1",
        "typescript": "~3.7.2"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "name": "css-analytics-frontend",
    "private": true,
    "scripts": {
        "build": "react-scripts build",
        "check": "npm run prettier-check && npm run eslint-check && npm run tsc-check",
        "eslint-check": "eslint --no-error-on-unmatched-pattern \"src/**/*.{ts,tsx}\"",
        "eslint-fix": "eslint --no-error-on-unmatched-pattern \"src/**/*.{ts,tsx}\" --fix",
        "fix": "npm run prettier-fix && npm run eslint-fix",
        "prettier-check": "prettier --config ./.prettierrc.json --check \"./src/**/*.{tx,tsx}\"",
        "prettier-fix": "prettier --config ./.prettierrc.json --write \"./src/**/*.{ts,tsx}\"",
        "tsc-check": "tsc --noEmit",
        "start": "cross-env PORT=8000 react-scripts start",
        "test": "react-scripts test"
    },
    "version": "0.1.0"
}
