    "eslintConfig": {
        "extends": "react-app"
    },
    "name": "css-analytics-frontend",
    "private": true,
    "scripts": {
        "build": "gatsby build",
        "check": "npm run prettier-check && npm run eslint-check && npm run tsc-check",
        "eslint-check": "eslint --no-error-on-unmatched-pattern \"src/**/*.{ts,tsx,js,jsx}\"",
        "eslint-fix": "eslint --no-error-on-unmatched-pattern \"src/**/*.{ts,tsx,js,jsx}\" --fix",
        "fix": "npm run prettier-fix && npm run eslint-fix",
        "prettier-check": "prettier --config ./.prettierrc.json --check \"./src/**/*.{tx,tsx,js,jsx,mdx}\"",
        "prettier-fix": "prettier --config ./.prettierrc.json --write \"./src/**/*.{ts,tsx,js,jsx,mdx}\"",
        "start": "cross-env PORT=8000 gatsby develop",
        "tsc-check": "tsc --noEmit"
    },
    "version": "0.1.0"
}
